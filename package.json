{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ua-trello-send",
  "title": "UA Trello Send",
  "description": "Send form data to Trello",
  "icon": "command-icon.png",
  "author": "anubis",
  "license": "MIT",
  "commands": [
    {
      "name": "arya",
      "title": "Arya",
      "description": "Create Trello Card for Arya in Web Support Board",
      "mode": "view"
    },
    {
      "name": "christine",
      "title": "Christine",
      "description": "Create Trello Card for Christine in Sauers Board",
      "mode": "view"
    },
    {
      "name": "kirsty",
      "title": "Kirsty",
      "description": "Create Trello Card for Kirsty in Sauers Board",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.78.1",
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "preferences": [
      {
        "description": "Trello API Key",
        "name": "apiKey",
        "required": true,
        "title": "Trello API Key",
        "type": "textfield"
      },
      {
        "description": "Trello Token",
        "name": "apiToken",
        "required": true,
        "title": "Trello API Token",
        "type": "textfield"
      },
      {
        "description": "Trello Board ID for Arya",
        "name": "boardIdArya",
        "required": true,
        "title": "Trello Board ID for Arya",
        "type": "textfield"
      },
      {
        "description": "Trello List ID for Christine",
        "name": "boardIdChristine",
        "required": true,
        "title": "Trello List ID for Christine",
        "type": "textfield"
      },
      {
        "description": "Trello List ID for Kirsty",
        "name": "listIdKirsty",
        "required": true,
        "title": "Trello List ID for Kirsty",
        "type": "textfield"
      },
      {
        "description": "Trello List ID for Arya",
        "name": "listIdArya",
        "required": true,
        "title": "Trello List ID for Arya",
        "type": "textfield"
      },
      {
        "description": "Trello User ID for Christine",
        "name": "userIdChristine",
        "required": true,
        "title": "Trello User ID for Christine",
        "type": "textfield"
      },
      {
        "description": "Trello User ID for Kirsty",
        "name": "userIdKirsty",
        "required": true,
        "title": "Trello User ID for Kirsty",
        "type": "textfield"
      },
      {
        "description": "Trello User ID for Anu",
        "name": "UserIdAnu",
        "required": true,
        "title": "Trello User ID for Anu",
        "type": "textfield"
      },
      {
        "description": "Trello User ID for Arya",
        "name": "userIdArya",
        "required": true,
        "title": "Trello User ID for Arya",
        "type": "textfield"
      }
    ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}